  <devices>
    <primary slot="any">
      <secondary slot="any">
        <MSXPiDevice id="MSXPi">
          <io base="0x56" num="8"/>
        </MSXPiDevice>
		<ROM id="MSXPi BIOS">
          <mem base="0x4000" size="0x4000"/>
          <rom>
            <sha1>5141771ce879b8f32f868bfacf0ac233475d8ff8</sha1>
            <filename>msxpibios.rom</filename>
          </rom>
        </ROM>
      </secondary>
    </primary>
  </devices>
</msxconfig>