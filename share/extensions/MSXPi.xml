<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>
  <info>
    <name>MSXPi</name>
    <manufacturer>https://github.com/costarc/MSXPi</manufacturer>
    <code/>
    <release_year/>
    <description>MSXPi Simulator, an extension that implements the MSXPi interface in the openMSX.</description>
    <type>pluggable</type>
